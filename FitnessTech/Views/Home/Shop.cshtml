@model IEnumerable<FitnessTech.Data.Entities.Product>

@{
    ViewData["Title"] = "Shop";
}

<h2>Shop</h2>


<table class="table table-striped table-hover">
    <thead>
        <tr>
                <th>
                    @Html.DisplayNameFor(model => model.Category)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Size)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Price)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Title)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.SupplementDescription)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.SupplementId)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Producer)
                </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Category)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Size)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Price)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Title)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SupplementDescription)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SupplementId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Producer)
            </td>
          
        </tr>
}
    </tbody>
</table>
